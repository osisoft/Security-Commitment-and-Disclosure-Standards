{
  "build": {
    "globalMetadata": {
      "_disableToc": true,
      "_disableBreadcrumb": true,
      "_enableSearch": true
    },
    "content": [
      {
        "files": [
          "content/**.md",
          "content/toc.yml",
          "toc.yml",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "content/images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "_exported_templates/default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
